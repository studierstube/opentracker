// -*- MPC -*-
// ASX_CCM_App.mpc,v 1.2 2003/12/23 20:23:21 dhinton Exp

project(*Lib) : acelib {
  sharedname = ccm_app
  Source_Files {
    CCM_App.cpp
  }
}

project(*Server) : aceexe {
  exename = server
  after += ASX_CCM_App_Lib
  Source_Files {
    SC_Server.cpp
  }
}

project(*Client) : aceexe {
  exename = client
  after += ASX_CCM_App_Server
  Source_Files {
    SC_Client.cpp
  }
}

